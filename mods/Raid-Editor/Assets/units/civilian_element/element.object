			<object name="g_head" enabled="true" cpy_material_vars="contour_color, contour_opacity">
				<material materials="m_martin"            max_distance="250" />
            	<material materials="m_martin_lod1"       max_distance="500" />
            	<material materials="m_martin_lod2" />
